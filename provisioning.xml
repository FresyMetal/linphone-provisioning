<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://www.linphone.org/xsds/lpconfig.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <!-- Assistant Configuration -->
  <section name="assistant">
    <entry name="domain">olivo.isrcomunicaciones.es</entry>
  </section>
  <section name="ui">
    <entry name="app_name">ISR Voip</entry>
    <entry name="primary_color">#003399</entry> <!-- Corporate Blue -->
    <entry name="secondary_color">#c0c0c0</entry> <!-- Corporate Silver -->
    <entry name="allow_edit_ui">0</entry>
    <!-- URL del logo en el repositorio de GitHub -->
    <entry name="company_logo_url">https://raw.githubusercontent.com/FresyMetal/sip/main/logo_isr.png</entry>
  </section>

  <!-- Audio Settings optimized for Spain/Europe -->
  <section name="audio">
    <entry name="codec_opus_preference">10</entry>
    <entry name="codec_pcmu_preference">0</entry>
  <section name="proxy_default_values">
    <entry name="reg_identity">sip:?@olivo.isrcomunicaciones.es</entry>
    <entry name="reg_proxy">&lt;sip:olivo.isrcomunicaciones.es;transport=udp&gt;</entry>
    <entry name="reg_route">&lt;sip:olivo.isrcomunicaciones.es;transport=udp&gt;</entry>
    <entry name="reg_sendregister">1</entry>
    <entry name="publish">0</entry>
    <entry name="dial_escape_plus">0</entry>
  </section>

  <section name="auth_info_0">
    <entry name="username">?</entry>
  </section>

</config>

